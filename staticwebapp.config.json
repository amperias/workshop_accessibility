{
  "trailingSlash": "never",
  "routes": [
    {
      "route": "/_astro/*",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable"
      }
    }
  ],
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self'; script-src 'unsafe-inline'"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_astro/*"]
  }
}
